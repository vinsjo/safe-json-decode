{
    "name": "safe-json-decode",
    "version": "1.0.2",
    "description": "a minimal package for encoding and decoding json without unhandled exceptions being thrown on failure",
    "author": "Vincent Sj√∂gren",
    "license": "MIT",
    "type": "module",
    "source": "./src/index.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.esm.js",
    "umd:main": "./dist/index.umd.js",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.esm.js",
            "default": "./dist/index.modern.js"
        }
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "microbundle",
        "dev": "microbundle watch",
        "update-browserlist": "npx browserslist@latest --update-db -y",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "microbundle": "latest"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/vinsjo/safe-json-decode"
    },
    "keywords": [
        "ESM",
        "CommonJS",
        "json",
        "decode",
        "encode"
    ]
}
