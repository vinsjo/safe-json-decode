{
    "name": "safe-json-decode",
    "version": "1.0.0",
    "description": "a minimal package for encoding and decoding json without unhandled exceptions being thrown on failure",
    "type": "module",
    "source": "./index.ts",
    "main": "./dist/index.js",
    "module": "./dist/index.m.js",
    "umd:main": "./dist/index.umd.js",
    "exports": {
        "require": "./dist/index.cjs",
        "default": "./dist/index.modern.js"
    },
    "types": "./dist/index.d.ts",
    "scripts": {
        "build": "microbundle",
        "dev": "microbundle watch"
    },
    "devDependencies": {
        "microbundle": "^0.15.1"
    },
    "author": "Vincent Sj√∂gren",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/vinsjo/safe-json-decode"
    },
    "keywords": [
        "ESM",
        "CommonJS",
        "json",
        "decode",
        "encode"
    ]
}
